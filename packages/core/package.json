{
  "name": "core",
  "version": "1.0.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*/index.mjs",
      "types": "./dist/*/index.d.ts"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "codegen": "dotenvx run -- graphql-codegen --config codegen.ts",
    "watch-codegen": "dotenvx run -- graphql-codegen --config codegen.ts --watch",
    "build": "rm -rf dist && tsup",
    "build-watch": "tsup --watch --no-dts",
    "clean": "rm -rf dist",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "biome lint --quit-on-problem .",
    "format": "biome format --write .",
    "test": "vitest --coverage"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.12.16",
    "@dotenvx/dotenvx": "^1.36.0",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typed-document-node": "^5.0.12",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.5.0",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/chrome": "^0.0.268",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@vitest/coverage-v8": "^2.1.8",
    "jsdom": "^25.0.1",
    "react": "^18.2.0",
    "semantic-release": "^24.2.9",
    "tsconfig": "workspace:*",
    "tsup": "^6.2.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@rollbar/react": "^0.12.1",
    "@tanstack/react-query": "^5.60.5",
    "graphql-request": "^7.1.2",
    "react-dom": "^18.2.0",
    "rollbar": "^2.26.4"
  }
}
